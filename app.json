{
  "expo": {
    "name": "Todo's",
    "slug": "staj_hafta_1_app",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/icon/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0714"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D0714"
      },
      "package": "com.bilalseen.staj_hafta_1_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/Open 24 Display St.ttf"]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "fe4d31cd-2edb-41ff-ba2d-0b2146c1f0be"
      }
    }
  }
}
